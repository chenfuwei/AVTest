#ifndef PrefixHeader_pch
#define PrefixHeader_pch

#define SCREEN_WIDTH [[UIScreen mainScreen] bounds].size.width
#define SCREEN_HEIGHT [[UIScreen mainScreen] bounds].size.height
#define LocalizedString(key) [[NSBundle mainBundle] localizedStringForKey:key value:@"" table:nil]
#define LocalizedTableString(key, tableName) [[NSBundle mainBundle] localizedStringForKey:key value:@"" table:tableName]
#define LocalizedTableInBundleString(key, tableName, bundle) NSLocalizedStringFromTableInBundle(key, tableName, bundle, @"")

#define MARGIN_TOP 64

